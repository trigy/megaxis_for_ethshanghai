    // chatgpt账号相关操作接口
    "PlusGPT":[
        {
            "name": "gpt提问",
            "description": "用户链接钱包后在主界面对chatgpt提问",
            "url": "/chatgpt/ask",
            "requestbody": {
                "content": [        // 当前界面的所有问题一起发送
                    {
                        "uid": 2000,
                        "data": [{
                            "type": 1, // int 1表示ai回答，2表示用户使用prompt提问，3表示用户使用content体温
                            "pid": "", // string
                            "params": ['s1', 's2'],
                            "content": "", //string
                            },
                            {}]
                    }
                ]  // 内容：历史提问，数组

            },
            "return-content":
            {
                "code": 1,                // int
                "message": "",                // string
                "data": "i don't know\n"  // 回答内容,markdown格式

            }
        },
        {
            "name": "调取已有的prompt参数",
            "description": "用户点击使用自己已有的prompt,获取该prompt的参数(或者直接调用prompt的接口)",
            "url": "/chatgpt/getPromptParams",
            "requestbody": {
                "uid": "",          // 用户id
                "pid": 2000         // prompt id
            },
            "return-content":
            {
                "code": 1,                // int
                "message": "",                // string
                "data": {
                    "params": ["params1", "params2"] // prompt参数
                }
            }
        },
    ],
